{
  "name": "@abstracter/atomic-release",
  "version": "0.0.0",
  "description": "Automated atomic release using the command pattern.",
  "private": false,
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/abstracter-io/atomic-release"
  },
  "scripts": {
    "outdated": "npx npm-check -u",
    "lint": "eslint --format pretty --ext=.js,.ts --fix",
    "lint:all": "npm run lint -- .",
    "lint:check": "eslint-config-prettier .eslintrc.js",
    "compile": "tsc --build tsconfig.compile.json --pretty",
    "postcompile": "node scripts/post-compile.js",
    "husky": "husky install",
    "test": "jest --projects .",
    "test:integration": "jest --projects src/__tests__/integration",
    "build": "npm run lint && npm run compile",
    "clean": "rimraf build",
    "clean:build": "npm run clean && npm run build",
    "commit": "git-cz",
    "release": "node scripts/release.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">= 12.18.0"
  },
  "keywords": [
    "release",
    "changelog",
    "atomic release",
    "command pattern",
    "semantic version",
    "version management",
    "conventional version",
    "conventional change log"
  ],
  "license": "MIT",
  "devDependencies": {
    "@abstracter/eslint-config": "^0.1.1",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.12.47",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "expect": "^27.3.0",
    "git-cz": "^4.7.6",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.1",
    "micromatch": "^4.0.4",
    "npm-check": "^5.9.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "await-to-js": "^3.0.0",
    "colors": "^1.4.0",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "conventional-changelog-writer": "^5.0.0",
    "conventional-commits-parser": "^3.2.2",
    "execa": "^4.0.3",
    "fetch-defaults": "^1.0.0",
    "get-stream": "^5.2.0",
    "into-stream": "^6.0.0",
    "mime": "^2.5.2",
    "node-fetch": "^2.6.2",
    "parse-author": "^2.0.0",
    "pretty-ms": "^7.0.1",
    "semver": "^7.3.5",
    "uri-templates": "^0.2.0"
  }
}
